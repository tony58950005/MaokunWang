#ifndef ERRORSTATE_H_
#define ERRORSTATE_H_

typedef enum
{
	ClockError,
	PWMError,
	ADCError,
	HighLevelCommError,
	UartError,
	AssertFailError,
	SpeedMeasurementError,
	ControlSpeedError,
	GetMotorSpeedError,
	SteeringError,
	MotorControlInitError,
	SteeringServoInit
} ErrorState;

#endif /* ERRORSTATE_H_ */
